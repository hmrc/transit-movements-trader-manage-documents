@(permission: viewmodels.PermissionToStartUnloading)

<fo:page-sequence master-reference="pages" initial-page-number="2" force-page-count="no-force"> <!-- SEQ 2 -->
    <fo:static-content flow-name="xsl-region-before">
        @header_table(permission)
    </fo:static-content>
    <fo:flow flow-name="xsl-region-body">
        <fo:block>
            @permission.goodsItems.toList.map(goodsItem => item.goods_item(goodsItem, permission))
        </fo:block>
        @*TODO: If NCTRetCopHEA104 is set this should be B, otherwise A (NCTRetCopHEA104 is only contained in an IE029 message)*@
        <fo:block id="A-last-page"/>
    </fo:flow>
</fo:page-sequence>