@(permission: viewmodels.PermissionToStartUnloading)

<fo:table-row height="8mm">
  <fo:table-cell>
    <fo:block/>
  </fo:table-cell>
  <fo:table-cell border-top-style="solid" border-top-width="1pt" border-top-color="black" border-left-style="solid" border-left-width="1pt" border-left-color="black">
    <fo:block/>
  </fo:table-cell>
  <fo:table-cell number-columns-spanned="4" border-style="solid" border-width="1pt" border-color="black">
    <fo:block>
      <fo:table width="100%" table-layout="fixed">
        <fo:table-column/>
        <fo:table-body>
          <fo:table-row height="4mm">
            <fo:table-cell display-align="center" padding-left="2mm">
              <fo:block>15 Country of dispatch/export</fo:block>
            </fo:table-cell>
          </fo:table-row>
          <fo:table-row height="4mm">
            <fo:table-cell text-align="center" font-weight="bold" display-align="center">
              <fo:block>
                @permission.singleCountryOfDispatch.map(country => country.code).getOrElse("---")
              </fo:block>
            </fo:table-cell>
          </fo:table-row>
        </fo:table-body>
      </fo:table>
    </fo:block>
  </fo:table-cell>
  <fo:table-cell>
    <fo:block/>
  </fo:table-cell>
</fo:table-row>
