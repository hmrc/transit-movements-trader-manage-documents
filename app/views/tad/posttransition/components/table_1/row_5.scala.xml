@(
    carrierIdentificationNumber: String,
    carrierContactPerson: String,
    additionalSupplyChainActorRoles: String,
    additionalSupplyChainActorIdentificationNumbers: String,
    departureTransportMeans: String,
    activeBorderTransportMeans: String,
    activeBorderTransportMeansConveyanceNumbers: String,
    placeOfLoading: String,
    placeOfUnloading: String,
    modeOfTransportAtBorder: String,
    inlandModeOfTransport: String,
    locationOfGoods: String,
    locationOfGoodsContactPerson: String
)

<fo:table-row>
    <fo:table-cell>
        <fo:block/>
    </fo:table-cell>

    <fo:table-cell border-right-style="solid" border-right-width="2pt" border-right-color="black"
                   border-left-style="solid" border-left-width="2pt" border-left-color="black"
                   border-bottom-style="solid" border-bottom-width="2pt" border-bottom-color="black"
                   display-align="center" padding-top="-50%">

        <fo:block-container
                font-weight="bold"
                reference-orientation="90" display-align="center"
                inline-progression-dimension.minimum="5%"
                inline-progression-dimension.maximum="auto"
                inline-progression-dimension.optimum="94mm">
            <fo:block margin-left="10%" font-size="7pt">TRANSIT ACCOMPANYING DOCUMENT</fo:block>
        </fo:block-container>
    </fo:table-cell>

    <fo:table-cell number-columns-spanned="6"
                   border-color="black" border-style="solid" border-width="1pt">
        <fo:block>
            <fo:table width="100%" table-layout="fixed">
                <fo:table-column column-number="1" column-width="25%"/>
                <fo:table-column column-number="2" column-width="25%"/>
                <fo:table-column column-number="3" column-width="25%"/>
                <fo:table-column column-number="4" column-width="25%"/>
                <fo:table-body>
                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>Carrier [13 12]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>ID @carrierIdentificationNumber</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>Place of loading [16 13]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>Location of goods [16 15]</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%" number-columns-spanned="2">
                            <fo:block>&#160;</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%" number-rows-spanned="3"
                                       border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>@placeOfLoading</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%" number-rows-spanned="4"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>@locationOfGoods</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%"
                                       border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt">
                            <fo:block>Contact person [13 02 074]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%"
                                       border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt">
                            <fo:block>@carrierContactPerson</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>Additional supply chain actor [13 14]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>ID @additionalSupplyChainActorIdentificationNumbers</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%" number-columns-spanned="2"
                                       border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt">
                            <fo:block>@additionalSupplyChainActorRoles</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>Place of unloading [16 14]</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>Departure transport means [19 05]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>&#160;</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%" number-rows-spanned="2"
                                       border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>@placeOfUnloading</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>Contact person [16 05 074]</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%" number-columns-spanned="2"
                                       border-bottom-color="black" border-bottom-style="solid" border-bottom-width="1pt">
                            <fo:block>@departureTransportMeans</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%" number-rows-spanned="4"
                                       border-left-color="black" border-left-style="solid" border-left-width="1pt">
                            <fo:block>@locationOfGoodsContactPerson</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>Active border transport means [19 08]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>&#160;</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%" number-rows-spanned="2"
                                       border-color="black" border-style="solid" border-width="1pt">
                            <fo:block>Mode of transport at the border [19 03]: @modeOfTransportAtBorder</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%" number-columns-spanned="2">
                            <fo:block>@activeBorderTransportMeans</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>Conveyance reference numbers [19 02]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>&#160;</fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%" number-rows-spanned="2"
                                       border-color="black" border-style="solid" border-width="1pt">
                            <fo:block>Inland mode of transport [19 04]: @inlandModeOfTransport</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%" number-columns-spanned="2">
                            <fo:block>@activeBorderTransportMeansConveyanceNumbers</fo:block>
                        </fo:table-cell>
                    </fo:table-row>
                </fo:table-body>
            </fo:table>
        </fo:block>
    </fo:table-cell>
</fo:table-row>