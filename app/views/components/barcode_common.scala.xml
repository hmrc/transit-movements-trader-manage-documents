@(
  message: String,
  height: String
)
<fo:instream-foreign-object fox:alt-text="Barcode">
  <barcode:barcode
      xmlns:barcode="http://barcode4j.krysalis.org/ns"
      message="@message">
    <barcode:code128>
      <barcode:height>@height</barcode:height>
      <barcode:module-width>0.24mm</barcode:module-width>
      <barcode:human-readable>none</barcode:human-readable>
    </barcode:code128>
  </barcode:barcode>
</fo:instream-foreign-object>
