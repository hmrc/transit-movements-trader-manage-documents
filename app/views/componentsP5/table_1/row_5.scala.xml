                <fo:table-body>
                    <fo:table-row>
                        <fo:table-cell>
                            <fo:block>
                                <fo:table width="100%" table-layout="fixed">

                                    <fo:table-body>
                                        <fo:table-row height="13.75mm">
                                            <fo:table-cell  alignment-adjust="auto" border-style="solid" border-width="1pt" border-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-column/>
                                                        <fo:table-column/>
                                                        <fo:table-body>
                                                            <fo:table-row height="4.5mm">
                                                                <fo:table-cell number-columns-spanned="1">
                                                                    <fo:block margin-top="0.5mm" margin-left="0.5mm" font-size="5pt">Place of loading [16 13]</fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                            <fo:table-row number-columns-spanned="4" height="4.5mm" padding-bottom="1mm">
                                                                <fo:table-cell padding-left="1mm">
                                                                    <fo:block>
                                                                        @ie029Data.placeOfLoading
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                        <fo:table-row height="13.75mm">
                                            <fo:table-cell  alignment-adjust="auto" border-style="solid" border-width="1pt" border-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-column/>
                                                        <fo:table-column/>
                                                        <fo:table-body>
                                                            <fo:table-row height="4.5mm">
                                                                <fo:table-cell number-columns-spanned="1">
                                                                    <fo:block margin-top="0.5mm" margin-left="0.5mm" font-size="5pt">Place of unloading [16 14]</fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                            <fo:table-row number-columns-spanned="4" height="4.5mm" padding-bottom="1mm">
                                                                <fo:table-cell padding-left="1mm">
                                                                    <fo:block>
                                                                        @ie029Data.placeOfUnloading
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                        <fo:table-row  height="4mm">
                                            <fo:table-cell  alignment-adjust="auto" border-style="solid" border-width="1pt" border-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-body>
                                                            <fo:table-row>
                                                                <fo:table-cell>
                                                                    <fo:block margin-top="0.5mm" margin-left="0.5mm" font-size="5pt">Mode of transport at border [19 03]</fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                        <fo:table-row  height="4mm">
                                            <fo:table-cell  alignment-adjust="auto" border-style="solid" border-width="1pt" border-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-body>
                                                            <fo:table-row>
                                                                <fo:table-cell>
                                                                    <fo:block margin-top="0.5mm" margin-left="0.5mm" font-size="5pt">Inland mode of transport [19 04]</fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                    </fo:table-body>
                                </fo:table>
                            </fo:block>
                        </fo:table-cell>
                        <fo:table-cell>
                            <fo:block>
                                <fo:table width="100%" table-layout="fixed">

                                    <fo:table-body>
                                        <fo:table-row>
                                            <fo:table-cell alignment-adjust="auto" border-style="solid" border-width="1pt" border-color="black">
                                                <fo:block>
                                                    <fo:table width="100%"  table-layout="fixed">
                                                        <fo:table-body>
                                                            <fo:table-row  height="18.2mm">
                                                                <fo:table-cell>
                                                                    <fo:block margin-top="0.5mm" margin-left="0.5mm" font-size="5pt">Location of goods [16 15]</fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                            <fo:table-row  height="18.2mm">
                                                                <fo:table-cell>
                                                                    <fo:block>
                                                                        <fo:block margin-top="0.5mm" margin-left="0.5mm" font-size="5pt">Contact person [16 15 074]</fo:block>
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                    </fo:table-body>
                                </fo:table>
                            </fo:block>
                        </fo:table-cell>

                    </fo:table-row>
                </fo:table-body>
            </fo:table>
        </fo:block>
    </fo:table-cell>



</fo:table-row>