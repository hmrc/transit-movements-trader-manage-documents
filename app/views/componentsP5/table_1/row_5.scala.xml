@import views.xml.componentsP5.table_1._

        @(viewModel: viewmodels.P5.TableViewModel)

<fo:table-row height="32%">
    <fo:table-cell>
        <fo:block/>
    </fo:table-cell>

    <fo:table-cell number-rows-spanned="1"
                   border-right-style="solid" border-right-width="1pt" border-right-color="black"
                   border-left-style="solid" border-left-width="1pt" border-left-color="black"
                   border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black"
                   display-align="center" padding-top="-50%">

        <fo:block-container
                font-weight="bold"
                font-size="7"
                reference-orientation="90" display-align="center"
                inline-progression-dimension.minimum="5%"
                inline-progression-dimension.maximum="auto"
                inline-progression-dimension.optimum="94mm"
                margin-bottom="10%m">
            <fo:block hyphenate="true" margin-left="10%">TRANSIT ACCOMPANYING DOCUMENT</fo:block>
        </fo:block-container>
    </fo:table-cell>


    <fo:table-cell height="2%" border-left-style="solid" border-left-width="1pt" border-left-color="black"
                   border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
        <fo:block>
            <fo:table width="100%" table-layout="fixed">
                <fo:table-column column-number="1" column-width="50%"/>
                <fo:table-column column-number="2" column-width="10%"/>
                <fo:table-column column-number="3" column-width="40%"/>
                <fo:table-body padding-bottom="4%">
                    <fo:table-row padding-bottom="4%">
                        <fo:table-cell display-align="center" margin-left="0.5%">
                            <fo:block>Carrier [13 12]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell display-align="center" margin-left="1%">
                            <fo:block>ID</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="4%">
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>
                            </fo:block>
                        </fo:table-cell>
                        <fo:table-cell number-columns-spanned="2" margin-left="0.5%">
                            <fo:block-container height="4%" overflow="hidden">
                                <fo:block>@viewModel.table1ViewModel.carrierIdentificationNumber</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="2%">
                        <fo:table-cell margin-left="0.5%" padding-top="2%">
                            <fo:block>
                                Contact person [13 06 074]
                            </fo:block>
                        </fo:table-cell>

                    </fo:table-row>


                    <fo:table-row padding-bottom="4%" border-top-style="solid" border-top-width="1pt" border-top-color="black">
                        <fo:table-cell display-align="center" margin-left="0.5%">
                            <fo:block>Additional supply chain actor [13 12]</fo:block>
                        </fo:table-cell>
                        <fo:table-cell display-align="center" margin-left="1%">
                            <fo:block>ID</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="4%">
                        <fo:table-cell margin-left="0.5%">
                            <fo:block>
                                <fo:block>@viewModel.table1ViewModel.additionalSupplyChainActorRoles</fo:block>
                            </fo:block>
                        </fo:table-cell>
                        <fo:table-cell number-columns-spanned="2" margin-left="0.5%" padding-bottom="4%">
                            <fo:block-container height="4%" overflow="hidden">
                                <fo:block>@viewModel.table1ViewModel.additionalSupplyChainActorIdentificationNumbers</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>



                    <!-- Departure transport means 8% -->

                    <fo:table-row height="2%" border-top-style="solid" border-top-width="1pt" border-top-color="black">
                        <fo:table-cell display-align="center" margin-left="0.5%">
                            <fo:block>Departure transport means [19 05]</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%" padding-bottom="4%">
                            <fo:block-container height="6%" overflow="hidden">
                                <fo:block>@viewModel.table1ViewModel.departureTransportMeans</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                    <!-- Active border transport mean 8% -->

                    <fo:table-row height="2%" border-top-style="solid" border-top-width="1pt" border-top-color="black">
                        <fo:table-cell display-align="center" margin-left="0.5%">
                            <fo:block>Active border transport means [19 08]</fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row>
                        <fo:table-cell margin-left="0.5%">
                            <fo:block-container height="4%" overflow="hidden">
                                <fo:block>@viewModel.table1ViewModel.activeBorderTransportMeans</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="2%">
                        <fo:table-cell margin-left="0.5%" padding-top="4%">
                            <fo:block>
                                Conveyance reference number [19 02]
                            </fo:block>
                        </fo:table-cell>
                        <fo:table-cell margin-left="0.5%" padding-top="4%">
                            <fo:block-container height="2%" overflow="hidden">
                                <fo:block>@viewModel.table1ViewModel.activeBorderTransportMeansConveyanceNumbers
                                </fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                </fo:table-body>
            </fo:table>
        </fo:block>
    </fo:table-cell>

    <fo:table-cell>
        <fo:block>
            <fo:table width="100%" table-layout="fixed">
                <fo:table-column column-width="86.15%"/>
                <fo:table-column column-width="86.15%"/>
