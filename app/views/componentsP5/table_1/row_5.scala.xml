                                                           border-left-width="1pt" border-left-color="black"
                                                           border-bottom-style="solid" border-bottom-width="1pt"
                                                           border-bottom-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-column/>
                                                        <fo:table-column/>
                                                        <fo:table-body>
                                                            <fo:table-row height="4%">
                                                                <fo:table-cell number-columns-spanned="1">
                                                                    <fo:block margin-top="0.5%" margin-left="0.5%">Place
                                                                        of unloading [16 14]
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                            <fo:table-row number-columns-spanned="4" height="20%"
                                                                          padding-bottom="1%">
                                                                <fo:table-cell margin-left="0.5%">
                                                                    <fo:block padding-bottom="33%">
                                                                        @viewModel.table1ViewModel.placeOfUnloading
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>

                                        <fo:table-row height="8%">
                                            <fo:table-cell alignment-adjust="auto" border-left-style="solid"
                                                           border-left-width="1pt" border-left-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-body>
                                                            <fo:table-row>
                                                                <fo:table-cell number-columns-spanned="2">
                                                                    <fo:block margin-left="0.5%">Mode of transport at
                                                                        border [19 03]
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                                <fo:table-cell display-align="center" margin-left="0.5"
                                                                               padding-bottom="5.5%">
                                                                    <fo:block>
                                                                        @viewModel.table1ViewModel.modeOfTransportAtBorder
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>

                                        <fo:table-row height="8%">
                                            <fo:table-cell alignment-adjust="auto" border-left-style="solid"
                                                           border-left-width="1pt" border-left-color="black"
                                                           border-top-style="solid" border-top-width="1pt"
                                                           border-top-color="black" border-bottom-style="solid"
                                                           border-bottom-width="1pt" border-bottom-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-body>
                                                            <fo:table-row>
                                                                <fo:table-cell number-columns-spanned="2">
                                                                    <fo:block margin-left="0.5%">Inland mode of
                                                                        transport [19 04]
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                                <fo:table-cell display-align="center" margin-left="0.5"
                                                                               padding-bottom="5.5%">
                                                                    <fo:block>
                                                                        @viewModel.table1ViewModel.inlandModeOfTransport
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                    </fo:table-body>
                                </fo:table>
                            </fo:block>
                        </fo:table-cell>

                        <fo:table-cell alignment-adjust="auto" border-left-style="solid" border-left-width="1pt"
                                       border-left-color="black" border-right-style="solid" border-right-width="1pt"
                                       border-right-color="black" border-bottom-style="solid" border-bottom-width="1pt"
                                       border-bottom-color="black">
                            <fo:block>
                                <fo:table width="100%" table-layout="fixed">
                                    <fo:table-body>
                                        <fo:table-row height="4%">
                                            <fo:table-cell number-columns-spanned="1">
                                                <fo:block margin-top="0.5%" margin-left="0.5%">Location of goods [16
                                                    15]
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                        <fo:table-row height="28%">
                                            <fo:table-cell margin-left="0.5%">
                                                <fo:block>
                                                    @viewModel.table1ViewModel.locationOfGoods
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                        <fo:table-row height="4%">
                                            <fo:table-cell padding-top="20%">
                                                <fo:block margin-top="0.5%" margin-left="0.5%">Contact person [16 15
                                                    074]
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                        <fo:table-row height="28%">
                                            <fo:table-cell margin-left="0.5%">
                                                <fo:block>
                                                    @viewModel.table1ViewModel.locationOfGoodsContactPerson
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>
                                    </fo:table-body>
                                </fo:table>
                            </fo:block>
                        </fo:table-cell>

                    </fo:table-row>
                </fo:table-body>
            </fo:table>
        </fo:block>
    </fo:table-cell>


</fo:table-row>