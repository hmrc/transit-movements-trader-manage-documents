                    <fo:table-row>
                        <fo:table-cell>
                            <fo:block>
                                <fo:table width="100%" table-layout="fixed">

                                    <fo:table-body>

                                        <fo:table-row height="200%">
                                            <fo:table-cell alignment-adjust="auto" border-left-style="solid"
                                                           border-left-width="1pt" border-left-color="black"
                                                           border-bottom-style="solid" border-bottom-width="1pt"
                                                           border-bottom-color="black">
                                                <fo:block>
                                                    <fo:table width="100%" table-layout="fixed">
                                                        <fo:table-column/>
                                                        <fo:table-column/>
                                                        <fo:table-body>
                                                            <fo:table-row height="4%">
                                                                <fo:table-cell number-columns-spanned="1">
                                                                    <fo:block margin-top="0.5%" margin-left="0.5%">Place
                                                                        of loading [16 13]
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                            <fo:table-row number-columns-spanned="4" height="10%"
                                                                          padding-bottom="1%">
                                                                <fo:table-cell margin-left="0.5%">
                                                                    <fo:block padding-bottom="33%">
                                                                        @viewModel.table1ViewModel.placeOfLoading
                                                                    </fo:block>
                                                                </fo:table-cell>
                                                            </fo:table-row>
                                                        </fo:table-body>
                                                    </fo:table>
                                                </fo:block>
                                            </fo:table-cell>
                                        </fo:table-row>

                                        <fo:table-row height="24%">
