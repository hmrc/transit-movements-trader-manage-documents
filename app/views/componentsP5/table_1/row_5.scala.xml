@import views.xml.componentsP5.table_1.carrier
@import views.xml.componentsP5.table_1._

@(ie029Data: models.P5.departure.IE029Data)

<fo:table-row height="32mm">
    <fo:table-cell>
        <fo:block/>
    </fo:table-cell>

    <fo:table-cell padding-top="1mm" text-align="center" vertical-align="top" border-top-color="black"
                   border-left-style="solid" border-left-width="1pt" border-left-color="black"
                   border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
        <fo:block/>
    </fo:table-cell>


    <fo:table-cell border-left-style="solid" border-left-width="1pt" border-left-color="black"
                   border-right-style="solid" border-right-width="1pt" border-right-color="black"
                   border-top-style="solid" border-top-width="1pt" border-top-color="black">
        <fo:block>
            <fo:table width="100%" table-layout="fixed">
                <fo:table-column column-number="1" column-width="44mm"/>
                <fo:table-column column-number="2" column-width="8mm"/>
                <fo:table-column column-number="3" column-width="37mm"/>
                <fo:table-body>

                    <!-- Carrier -->

                    <fo:table-row height="3mm" border-top-style="solid" border-top-width="1pt" border-top-color="black">
                        <fo:table-cell display-align="center" padding-left="1mm">
                            <fo:block>Carrier [13 12]</fo:block>
                        </fo:table-cell>

                        <fo:table-cell display-align="center" padding-left="2mm">
                            <fo:block>ID</fo:block>
                        </fo:table-cell>

                        <fo:table-cell display-align="center" padding-left="2mm" font-weight="bold">
                            <fo:block/>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="3mm">
                        <fo:table-cell padding-left="1mm">
                            <fo:block>
                                @ie029Data.carrier
                            </fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="3mm">
                        <fo:table-cell padding-left="1mm">
                            <fo:block>
                                Contact person [13 06 074]
                            </fo:block>
                        </fo:table-cell>

                    </fo:table-row>

                    <!--        Additional supply chain actor-->

                    <fo:table-row height="3mm" border-top-style="solid" border-top-width="1pt" border-top-color="black">
                        <fo:table-cell display-align="center" padding-left="1mm">
                            <fo:block>Additional supply chain actor [13 12]</fo:block>
                        </fo:table-cell>

                        <fo:table-cell display-align="center" padding-left="2mm">
                            <fo:block>ID</fo:block>
                        </fo:table-cell>

                        <fo:table-cell display-align="center" padding-left="2mm">
                            <fo:block/>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="6mm">
                        <fo:table-cell padding-left="1mm">
                            <fo:block>
                                @ie029Data.additionalSupplyChainActor
                            </fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <!--        Departure transport means -->

                    <fo:table-row height="3mm" border-top-style="solid" border-top-width="1pt" border-top-color="black">
                        <fo:table-cell display-align="center" padding-left="1mm">
                            <fo:block>Departure transport means [19 05]</fo:block>
                        </fo:table-cell>

                        <fo:table-cell display-align="center" padding-left="2mm">
                            <fo:block/>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="6mm">
                        <fo:table-cell padding-left="1mm">
                            <fo:block>
                                @ie029Data.departureTransportMeans
                            </fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <!--       Active border transport means -->


                    <fo:table-row height="3mm" border-top-style="solid" border-top-width="1pt" border-top-color="black">
                        <fo:table-cell display-align="center" padding-left="1mm">
                            <fo:block>Active border transport means [19 08]</fo:block>
                        </fo:table-cell>

                        <fo:table-cell display-align="center" padding-left="2mm" font-weight="bold">
                            <fo:block/>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="3mm">
                        <fo:table-cell padding-left="1mm">
                            <fo:block>
                                @ie029Data.activeBorderTransportMeans
                            </fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                    <fo:table-row height="2mm" border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
                        <fo:table-cell padding-left="1mm">
                            <fo:block>
                                Conveyance reference number [19 02]
                            </fo:block>
                        </fo:table-cell>
                    </fo:table-row>

                </fo:table-body>
            </fo:table>
        </fo:block>
    </fo:table-cell>

    <fo:table-cell>
        <fo:block>
            <fo:table width="100%" table-layout="fixed">
                <fo:table-column column-width="47.5mm"/>
