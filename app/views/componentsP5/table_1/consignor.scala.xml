@(consignor: String, identificationNumber: String, consignorContactPerson: String)

<fo:table-cell border-left-style="solid" border-left-width="1pt" border-left-color="black" border-right-style="solid" border-right-width="1pt" border-right-color="black" border-top-style="solid" border-top-width="1pt" border-top-color="black"  border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
  <fo:block>
    <fo:table width="100%" table-layout="fixed">
      <fo:table-column column-number="1" column-width="50%"/>
      <fo:table-column column-number="2" column-width="10%"/>
      <fo:table-column column-number="3" column-width="40%"/>
      <fo:table-body>
        <fo:table-row height="98%">
          <fo:table-cell display-align="center" margin-left="0.5%">
            <fo:block>Consignor [13 02]</fo:block>
          </fo:table-cell>

          <fo:table-cell display-align="center" margin-left="1%">
            <fo:block>ID</fo:block>
          </fo:table-cell>

        </fo:table-row>

        <fo:table-row >
          <fo:table-cell margin-left="0.5%">
            <fo:block-container height="2%" overflow="hidden">
              <fo:block>@consignor</fo:block>
            </fo:block-container>
          </fo:table-cell>

          <fo:table-cell number-columns-spanned="2" margin-left="0.5%">
            <fo:block-container overflow="hidden">
              <fo:block>@identificationNumber</fo:block>
            </fo:block-container>
          </fo:table-cell>

        </fo:table-row>

        <fo:table-row height="2%">
          <fo:table-cell margin-left="0.5%" padding-top="11%" >
            <fo:block >
              Contact person [13 02 074]
            </fo:block>
          </fo:table-cell>

          <fo:table-cell number-columns-spanned="2" margin-left="0.5%" padding-top="11%">
            <fo:block-container overflow="hidden">
              <fo:block>@consignorContactPerson</fo:block>
            </fo:block-container>
          </fo:table-cell>

        </fo:table-row>
      </fo:table-body>
    </fo:table>
  </fo:block>
</fo:table-cell>
