@import views.xml.unloadingPermissionComponentsP5.table_1._
        @import views.xml.unloadingPermissionComponentsP5._

        @(ie043Data: models.P5.unloading.IE043Data)


<fo:table-row height="30mm" font-size = "5.5pt">
    <fo:table-cell>
        <fo:block/>
    </fo:table-cell>

    <fo:table-cell >
        <fo:block />
    </fo:table-cell>

    <fo:table-cell border-left-style="solid" border-left-width="1pt" border-left-color="black"
                   border-right-style="solid" border-right-width="1pt" border-right-color="black"
                   border-top-style="solid" border-top-width="1pt" border-top-color="black"
                   border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black"
                   number-columns-spanned="6"

    >
        <fo:block>
            <fo:table width="100%" table-layout="fixed">
                <fo:table-column column-number="1" column-width="35mm"/>
                <fo:table-column column-number="2" column-width="142mm" />
                <fo:table-body >

                    <!-- Supporting document row -->

                    <fo:table-row height="3mm">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block><fo:inline font-weight="bold">Supporting document</fo:inline> [1203]</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>
                    <fo:table-row height="7mm" border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block>@(ie043Data.supportingDocument)</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                    <!-- Additional reference row -->

                    <fo:table-row height="3mm">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block><fo:inline font-weight="bold">Additional reference</fo:inline> [1204]</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>
                    <fo:table-row height="7mm" border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block>@(ie043Data.additionalReference)</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                    <!-- Additional information row -->

                    <fo:table-row height="3mm">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block><fo:inline font-weight="bold">Additional information </fo:inline> [1202]</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>
                    <fo:table-row height="7mm" border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block>@(ie043Data.additionalInformation)</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                    <!-- Customs office of destination row -->

                    <fo:table-row height="3mm">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block><fo:inline font-weight="bold">Customs office of destination</fo:inline> [1705]</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>
                    <fo:table-row height="7mm" border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block>@(ie043Data.customsOfficeOfDestination)</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>

                    <!-- Country of destination row -->

                    <fo:table-row height="3mm">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block><fo:inline font-weight="bold">Country of destination</fo:inline> [1607]</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>
                    <fo:table-row height="7mm" border-bottom-style="solid" border-bottom-width="1pt" border-bottom-color="black">
                        <fo:table-cell  margin-left="0.5mm">
                            <fo:block-container overflow="hidden" margin-top = "0.8mm">
                                <fo:block>@(ie043Data.countryOfDestination)</fo:block>
                            </fo:block-container>
                        </fo:table-cell>
                    </fo:table-row>
                    
                </fo:table-body>
            </fo:table>
        </fo:block>
    </fo:table-cell>

</fo:table-row>
