@import views.xml.unloadingPermissionComponentsP5.items._

@this()

@(items: Seq[String], mrn: String)

<fo:page-sequence master-reference="pages" initial-page-number="2" force-page-count="no-force">
    <fo:static-content flow-name="xsl-region-before">
        @header(mrn)
    </fo:static-content>
    <fo:flow flow-name="xsl-region-body">
        <fo:block>
            @items.map(item(_))
        </fo:block>
    </fo:flow>
</fo:page-sequence>
