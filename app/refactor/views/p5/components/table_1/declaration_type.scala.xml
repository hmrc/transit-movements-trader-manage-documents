@(declarationType: String)
<fo:table-cell border-right-style="solid" border-right-width="1pt" border-right-color="black">
    <fo:block>
        <fo:table width="100%" table-layout="fixed">
            <fo:table-column/>
            <fo:table-column/>
            <fo:table-body>
                <fo:table-row height="2%">
                    <fo:table-cell number-columns-spanned="2" text-align="left" display-align="left"
                                   padding-bottom="20%">
                        <fo:block margin-left="3.5%" font-size="5pt">Type [1101]</fo:block>
                    </fo:table-cell>
                </fo:table-row>
                <fo:table-row number-columns-spanned="2" height="6%" border-bottom-style="solid"
                              border-bottom-width="1pt" border-bottom-color="black" padding-bottom="10%">
                    <fo:table-cell margin-left="3.5%" text-align="center" display-align="center">
                        <fo:block>
                            @declarationType
                        </fo:block>
                    </fo:table-cell>
                </fo:table-row>
            </fo:table-body>
        </fo:table>
    </fo:block>
</fo:table-cell>

