@import refactor.viewmodels.p4.tad.Table1ViewModel.ConsignorViewModel


@(
carrierIdentificationNumber: String
)

<fo:table-cell border-left-style="solid" border-left-width="1pt" border-left-color="black" border-right-style="solid" border-right-width="1pt" border-right-color="black" border-top-style="solid" border-top-width="1pt" border-top-color="black">
  <fo:block>
    <fo:table width="100%" table-layout="fixed">
      <fo:table-column column-number="1" column-width="44mm"/>
      <fo:table-column column-number="2" column-width="8mm"/>
      <fo:table-column column-number="3" column-width="37mm"/>
      <fo:table-body>
        <fo:table-row height="5.33mm">
          <fo:table-cell display-align="center" padding-left="2mm">
            <fo:block>CARRIER <fo:inline font-size="6px">(S07)</fo:inline>  [1312]</fo:block>
          </fo:table-cell>
        </fo:table-row>
        <fo:table-row height="5.33mm">
          <fo:table-cell display-align="center" padding-left="2mm">
            <fo:block>Identification Number</fo:block>
          </fo:table-cell>
        </fo:table-row>
        <fo:table-row height="5.33mm">
          <fo:table-cell  padding-left="2mm" font-weight="bold">
            <fo:block>
              @carrierIdentificationNumber
            </fo:block>
          </fo:table-cell>
        </fo:table-row>
      </fo:table-body>
    </fo:table>
  </fo:block>
</fo:table-cell>
