@import refactor.viewmodels.p4.tad.Table1ViewModel.ConsignorViewModel

@(
  vm: ConsignorViewModel
)

<fo:table-row height="5.33mm">
  <fo:table-cell display-align="center" padding-left="2mm">
    <fo:block>2 Consignor/Exporter</fo:block>
  </fo:table-cell>
  <fo:table-cell display-align="center" padding-left="2mm">
    <fo:block>No &#160; <fo:inline font-weight = "bold">@vm.eori</fo:inline></fo:block>
  </fo:table-cell>
  <fo:table-cell display-align="center" padding-left="2mm" font-weight="bold">
    <fo:block>
      @vm.streetAndNumber
    </fo:block>
  </fo:table-cell>
</fo:table-row>
<fo:table-row height="5.33mm">
<fo:table-cell  display-align="center" padding-left="2mm" font-weight="bold">
  <fo:block>
    @vm.name
  </fo:block>
</fo:table-cell>
</fo:table-row>
<fo:table-row height="5.33mm">
<fo:table-cell display-align="center" padding-left="2mm"  font-weight="bold">
  <fo:block>
    @vm.city
  </fo:block>
</fo:table-cell>
<fo:table-cell display-align="center" padding-left="2mm"  font-weight="bold">
  <fo:block>
    @vm.postcode
  </fo:block>
</fo:table-cell>
<fo:table-cell display-align="center" padding-left="2mm" font-weight="bold">
  <fo:block>
    @vm.country
  </fo:block>
</fo:table-cell>
</fo:table-row>
